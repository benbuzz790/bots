[flake8]
max-line-length = 127
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    docs,
    build,
    dist,
    .eggs,
    *.egg-info,
    .venv,
    tests/test_decorators.py,
   issue103,
   tests/test_python_edit.py,
   tests/test_terminal_tools.py
per-file-ignores =
    __init__.py:F401
